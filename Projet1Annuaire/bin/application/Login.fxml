<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="272.0" prefWidth="406.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControllerLogin">
   <children>
      <TextField fx:id="tfLogin" layoutX="68.0" layoutY="43.0" prefHeight="45.0" prefWidth="270.0" promptText="Nom d'utilisateur">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="tfMotPasse" layoutX="68.0" layoutY="110.0" prefHeight="45.0" prefWidth="270.0" promptText="Mot de passe">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnLogin" layoutX="68.0" layoutY="181.0" mnemonicParsing="false" onAction="#login" prefHeight="45.0" prefWidth="121.0" text="ok">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="btnAnnuler" layoutX="209.0" layoutY="181.0" mnemonicParsing="false" onAction="#fermerLogin" prefHeight="45.0" prefWidth="121.0" text="Annuler">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
